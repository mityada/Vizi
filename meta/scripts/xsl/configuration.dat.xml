<root>
    <node name="visualizer">
        <param-i18n name="algorithm" attr="name"/>
        <param-i18n name="author" attr="author"/>
        <param-i18n name="supervisor" attr="supervisor"/>
        <param-i18n name="copyright" attr="copyright"/>
        <param name="author-email" attr="author-email"/>
        <param name="supervisor-email" attr="supervisor-email"/>
    </node>
    <node name="font">
        <param name="face" attr="face"/>
        <param name="size" attr="size"/>
        <param name="style" attr="style"/>
    </node>
    <node name="color">
        <param attr="value"/>
    </node>
    <node name="panel">
        <param name="foreground" attr="foreground"/>
        <param name="background" attr="background"/>
    </node>
    <node name="style">
        <param name="text-color" attr="text-color"/>
        <param name="text-align" attr="text-align"/>
        <param name="message-align" attr="message-align"/>
        <param name="border-color" attr="border-color"/>
        <param name="border-status" attr="border-status"/>
        <param name="fill-color" attr="fill-color"/>
        <param name="fill-status" attr="fill-status"/>
        <param name="aspect" attr="aspect"/>
        <param name="aspect-status" attr="aspect-status"/>
        <param name="padding" attr="padding"/>
    </node>
    <node name="button">
        <param-i18n attr="caption"/>
        <param-i18n name="hint" attr="hint"/>
        <param name="hotKey" attr="hotKey"/>
    </node>
    <node name="spin-panel">
        <param-i18n name="caption" attr="caption"/>
        <param-i18n name="hint" attr="hint"/>
        <param name="value" attr="value"/>
        <param name="value-max" attr="max-value"/>
        <param name="value-min" attr="min-value"/>
        <param name="step" attr="step"/>
        <param name="big-step" attr="big-step"/>
        <param name="big-step-delay" attr="big-step-delay"/>
    </node>
    <node name="adjustablePanel">
        <param-i18n name="caption" attr="caption"/>
        <param-i18n name="hint" attr="hint"/>
        <param name="value" attr="value"/>
        <param name="maximum" attr="maximum"/>
        <param name="minimum" attr="minimum"/>
        <param name="unitIncrement" attr="unitIncrement"/>
        <param name="blockIncrement" attr="blockIncrement"/>
        <param name="blockInterval" attr="blockInterval"/>
    </node>
    <node name="property">
        <param attr="value"/>
    </node>
    <node name="message">
        <param-i18n attr="message"/>
    </node>
    <node name="choice">
        <param-i18n name="hint" attr="hint"/>
    </node>
</root>